{
  "name": "kv-keeper",
  "version": "1.0.0",
  "description": "Key-value storage for JS that wraps IndexedDB with fallback to LocalStorage",
  "main": "dist/kv-storage.min.js",
  "scripts": {
    "deps": "npm install && bower install",
    "dist": ".scripts/dist.sh",
    "lint": "eslint .; jscs .",
    "test": "mocha-phantomjs --reporter dot test/specs/test.html && mocha-phantomjs --reporter dot test/specs/test-dist.html",
    "web-server": "php -S localhost:8000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andre487/kv-keeper.js.git"
  },
  "keywords": [
    "localstorage",
    "indexeddb",
    "key-value"
  ],
  "author": "Andrey Prokopyuk <andrey.prokopyuk@gmail.com> (http://andre.life)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andre487/kv-keeper.js/issues"
  },
  "homepage": "https://github.com/andre487/kv-keeper.js#readme",
  "devDependencies": {
    "bower": "^1.6.5",
    "doctoc": "coryfklein/doctoc",
    "eslint": "^1.9.0",
    "git-hooks": "^1.0.2",
    "jscs": "^2.5.1",
    "mocha-phantomjs": "^4.0.1",
    "phantomjs": "^1.9.18",
    "uglify-js": "^2.6.1"
  }
}
